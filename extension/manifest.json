{
    "manifest_version": 2,

    "name": "Instagram Followers Bot",
    "description": "",
    "version": "1.0",

    "icons": {
        "16": "./assets/icon_16.png",
        "32": "./assets/icon_32.png",
        "48": "./assets/icon_48.png"
    },

    "browser_action": {
        "default_icon": "./assets/icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "notifications",
        "http://*/",
        "https://*/"
    ]
}
